{
	"private": true,
	"name": "buffet",
	"version": "0.2.0",
	"description": "testing",
	"bin": {
		"ff": "./dist/index.js"
	},
	"types": "./dist/index.d.ts",
	"scripts": {
		"prebuild": "pnpm run clean",
		"pretest": "pnpm run lint",
		"clean": "rimraf ./dist",
		"lint": "eslint ./src/**/*.ts",
		"build": "tsc",
		"test": "mocha ./test/**/*Tests.ts",
		"server-build": "env DEBUG=buffet:* ts-node ./src/index.ts server build",
		"server-serve": "env DEBUG=buffet:* ts-node ./src/index.ts server serve",
		"remotes": "env DEBUG=buffet:* ts-node ./src/index.ts remotes",
		"remote-add": "env DEBUG=buffet:* ts-node ./src/index.ts remote add",
		"remote-del": "env DEBUG=buffet:* ts-node ./src/index.ts remote del",
		"remote-info": "env DEBUG=buffet:* ts-node ./src/index.ts remote info",
		"bottle-add": "env DEBUG=buffet:* ts-node ./src/index.ts bottle add",
		"bottle-del": "env DEBUG=buffet:* ts-node ./src/index.ts bottle del",
		"bottle-find": "env DEBUG=buffet:* ts-node ./src/index.ts bottle find",
		"bottle-info": "env DEBUG=buffet:* ts-node ./src/index.ts bottle info",
		"add": "env DEBUG=buffet:* ts-node ./src/index.ts add",
		"del": "env DEBUG=buffet:* ts-node ./src/index.ts del",
		"find": "env DEBUG=buffet:* ts-node ./src/index.ts find",
		"info": "env DEBUG=buffet:* ts-node ./src/index.ts info"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"chalk": "^3.0.0",
		"chokidar": "^3.3.1",
		"debug": "^4.1.1",
		"exec-root": "^1.0.9",
		"express": "^4.17.1",
		"fast-glob": "^3.2.2",
		"find-up": "^4.1.0",
		"fuse.js": "^5.1.0",
		"humanize-duration": "^3.22.0",
		"lodash": "^4.17.15",
		"mkdirp": "^1.0.4",
		"ora": "^4.0.3",
		"prompts": "^2.3.2",
		"rimraf": "^3.0.2",
		"semver": "^7.2.1",
		"supports-color": "^7.1.0",
		"ts-node": "^8.8.2",
		"typescript": "^3.8.3",
		"yargs": "^15.3.1"
	},
	"devDependencies": {
		"@types/chai": "^4.2.11",
		"@types/chai-as-promised": "^7.1.2",
		"@types/debug": "^4.1.5",
		"@types/express": "^4.17.4",
		"@types/humanize-duration": "^3.18.0",
		"@types/lodash": "^4.14.149",
		"@types/mkdirp": "^1.0.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/prompts": "^2.0.5",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.1.0",
		"@types/yargs": "^15.0.4",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/eslint-plugin-tslint": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^6.8.0",
		"mocha": "^7.1.1",
		"mocha-typescript": "^1.1.17"
	}
}
